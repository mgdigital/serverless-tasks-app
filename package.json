{
  "name": "dataminr",
  "version": "1.0.1",
  "main": "src/handler.js",
  "license": "MIT",
  "scripts": {
    "deploy:env": "sls deploy --stage $NODE_ENV",
    "deploy": "export NODE_ENV=dev && yarn deploy:env",
    "deploy:production": "export NODE_ENV=production && yarn deploy:env",
    "deploy:stage": "export NODE_ENV=stage && yarn deploy:env",
    "lint": "eslint src --ext ts,js --no-ignore",
    "lint:fix": "yarn lint --fix",
    "serve": "sls offline",
    "serve:watch": "nodemon -e js,ts,jsx,tsx -x yarn serve",
    "tail:hello": "sls logs --function hello --tail",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "sls webpack",
    "watch:hello": "sls invoke local --watch --function hello --path fixtures/event.json",
    "db:initialize": "node scripts/initializeDB.js"
  },
  "dependencies": {
    "ajv": "^8.6.2",
    "pg": "^8.7.1",
    "uuid": "^8.3.2",
    "yesql": "^5.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.82",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "@types/pg": "^8.6.1",
    "@types/uuid": "^8.3.1",
    "@types/yesql": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "inquirer": "^8.1.2",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "serverless": "^2.55.0",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-offline": "^8.0.0",
    "serverless-plugin-warmup": "^5.3.1",
    "serverless-prune-plugin": "^1.5.1",
    "serverless-webpack": "^5.5.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "webpack": "^5.51.0",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  }
}
